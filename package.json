{
  "name": "js-jam",
  "version": "0.1.1",
  "description": "A set of utilities for conveniently working with JSON-API data.",
  "esnext": "src/index.js",
  "main": "dist/index.js",
  "files": [
    "src/**"
  ],
  "scripts": {
    "build": "webpack --mode development --progress --colors",
    "watch": "webpack --mode development --progress --colors --watch",
    "build:prod": "webpack --mode production --progress --colors",
    "watch:prod": "webpack --mode production --progress --colors --watch",
    "test": "mocha --require babel-polyfill --compilers js:babel-register -R nyan --recursive tests/*.js",
    "test:one": "mocha --require babel-polyfill --compilers js:babel-register -R nyan"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uptick/jam.git"
  },
  "author": "Uptick Pty Ltd",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "code": "^5.2.0",
    "eslint": "^4.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.2.0",
    "peer-deps-externals-webpack-plugin": "^1.0.2",
    "sinon": "^4.4.2",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "peerDependencies": {
    "js-tinyapi": "^0.2.0",
    "moment": "^2.21.0",
    "uuid": "^2.0.3"
